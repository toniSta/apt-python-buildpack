#!/bin/bash


echo "----- Installing GhostScript 9.14 -------"


tar xf $ROOT_DIR/ghostscript.tar.bz2
cd ghostscript
echo "Start ./configure"
./configure -q --disable-cups --disable-gtk --with-drivers=FILES
echo "Finished ./configure"
echo "Start make"
make -s
echo "Finished make"
# cd $ROOT_DIR

echo "----- Finished installion of GhostScript 9.14 -------"
